{
	"name" : "omez/doctrine2zf",
	"type" : "library", 
	"description" : "Doctrine2 module for Zend Framework 2. Compilation of rescent modules + migrations stuff", 
	"license" : "MIT", 
	"keywords" : [
		"doctrine", 
		"module", 
		"zf2", 
		"orm",
		"migrations",
		"data-fixtures"
	], 
	"authors" : [
		{
			"name" : "Alexander Sergeychik", 
			"email" : "alexander.sergeychik@gmail.com", 
			"homepage" : "http://sergeychik.com"
		}
	], 
	"require" : {
		"php" : ">=5.3", 
		"doctrine/common" : "2.*", 
		"doctrine/dbal" : "2.*",
		"doctrine/orm" : "2.*", 
		
		"doctrine/migrations@dev": "dev-master",
		"doctrine/data-fixtures@dev": "dev-master",
		
		"symfony/yaml" : "2.*", 
		"symfony/console" : "2.*"
	}, 
	"autoload" : {
		"psr-0" : {
			"DoctrineModule" : "src/", 
			"DoctrineModuleTest" : "tests/", 
			"DoctrineORMModule" : "src/", 
			"DoctrineORMModuleTest" : "tests/"
		}
	}, 
	"bin" : [
		"bin/doctrine"
	]
	
}