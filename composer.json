{
	"name": "omez/doctrine2zf",
	"type": "library",
	"version": "0.1.2-beta2",
	"description": "Doctrine2 module for Zend Framework 2. Compilation of rescent modules + migrations stuff",
	"license": "MIT",
	"keywords": [
		"doctrine",
		"module",
		"zf2",
		"orm",
		"migrations",
		"data-fixtures"
	],
	"authors": [
		{
			"name": "Alexander Sergeychik",
			"email": "alexander.sergeychik@gmail.com",
			"homepage": "http://sergeychik.com"
		}
	],
	"target-dir": "modules/Doctrine2zf",
	
	"autoload": {
		"psr-0": {
			"DoctrineModule": "src/",
			"DoctrineModuleTest": "tests/",
			"DoctrineORMModule": "src/",
			"DoctrineORMModuleTest": "tests/"
		}
	},
	
	"bin": [
		"bin/doctrine"
	],
	
	"require": {
		"php": ">=5.3",
		"doctrine/common": "2.*",
		"doctrine/dbal": "2.*",
		"doctrine/orm": "2.*",
		"doctrine/migrations": "dev-master",
		"doctrine/data-fixtures": "dev-master",
		"symfony/yaml": "2.*",
		"symfony/console": "2.*",
		
		"zendframework/zendframework": ">=2.0.0-RC1"
	}
	
}